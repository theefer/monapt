{
  "name": "monapt",
  "version": "1.0.0",
  "description": "Type abstractions for TypeScript and JavaScript",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "lint:ts": "tslint --force --format verbose --project ./tsconfig.json --type-check './src/**/*.ts' './test/**/*.ts'",
    "smoke:ts": "tsc --noEmit --pretty",
    "test": "webpack --config test.config.js && ava --verbose",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiaweihli/monapt.git"
  },
  "keywords": [
    "option",
    "future",
    "try",
    "monad",
    "functional",
    "util",
    "scala",
    "type",
    "types"
  ],
  "author": "Kevin Li",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jiaweihli/monapt/issues"
  },
  "homepage": "https://github.com/jiaweihli/monapt#readme",
  "devDependencies": {
    "ava": "^0.17.0",
    "glob": "^7.1.1",
    "ts-loader": "^1.3.3",
    "tslint": "^4.2.0",
    "typescript": "^2.1.4",
    "webpack": "^2.2.0-rc.3"
  }
}
